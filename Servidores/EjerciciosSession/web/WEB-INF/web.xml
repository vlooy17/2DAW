<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Ejercicio1</servlet-name>
        <servlet-class>Session.Ejercicio1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cerrarSesion</servlet-name>
        <servlet-class>Session.cerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>primeraPag</servlet-name>
        <servlet-class>Session.primeraPag</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>segundoPag</servlet-name>
        <servlet-class>Session.segundoPag</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>terceraPag</servlet-name>
        <servlet-class>Session.terceraPag</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ejercicio1</servlet-name>
        <url-pattern>/Ejercicio1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cerrarSesion</servlet-name>
        <url-pattern>/cerrarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>primeraPag</servlet-name>
        <url-pattern>/primeraPag</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>segundoPag</servlet-name>
        <url-pattern>/segundoPag</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>terceraPag</servlet-name>
        <url-pattern>/terceraPag</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
